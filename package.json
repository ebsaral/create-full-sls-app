{
  "name": "create-full-sls-app",
  "description": "Serverless boilerplate CLI tool: GitLab and GitHub CI/CD, DataDog, Multi-Region, and many more setups. Ready-to-deploy in a few seconds!",
  "version": "1.5.0",
  "main": "src/index.js",
  "author": "Emin Bugra Saral <combats.refit-03@icloud.com> (https://saral.dev/)",
  "funding": {
    "type": "individual",
    "url": "https://paypal.me/ebsaral"
  },
  "types": "src/index.d.ts",
  "prepare": "npm run build",
  "postversion": "git push && git push --tags",
  "scripts": {
    "dev": "yarn build && yarn start",
    "start": "node dist/index.js",
    "build": "tsc && shx rm -rf dist/templates && shx cp -r templates dist"
  },
  "bin": "./dist/index.js",
  "repository": "https://github.com/ebsaral/create-full-sls-app",
  "license": "MIT",
  "url": "https://github.com/ebsaral/create-full-sls-app/issues",
  "keywords": [
    "serverless",
    "gitlab",
    "github",
    "datadog",
    "sonarcloud",
    "sonarqube",
    "yup",
    "aws",
    "lambda",
    "aws-lambda",
    "typescript",
    "serverless-app",
    "serverless-app-builder",
    "serverless-app-creator",
    "serverless-app-creator-cli",
    "serverless-app-creator-cli-app"
  ],
  "devDependencies": {
    "@types/ejs": "^3.1.0",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^17.0.4",
    "@types/shelljs": "^0.8.10",
    "@types/yargs": "^17.0.8",
    "shx": "^0.3.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "inquirer": "^8.2.0",
    "shelljs": "^0.8.4",
    "yargs": "^17.3.1"
  }
}
